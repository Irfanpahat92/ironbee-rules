<?xml version="1.0" encoding="UTF-8"?>
<rxb:regexbuddy xmlns:rxb="http://www.regexbuddy.com/regexbuddy32.xsd" version="3.2">
	<libraryfile>
		<action id="DataHostname" flavor="pcre" flavorreplace="jgsoft" testscope="line">
			<regex>^(?i)[-_.:a-z0-9]{1,255}$</regex>
			<description>Data: Hostname</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="DataIPaddressIPv4orIPv6" flavor="pcre" flavorreplace="jgsoft" testscope="line">
			<regex>(?i)^(?:[0-9.]+|\[[:a-z0-9]+\])$</regex>
			<description>Data: IP address (IPv4 or IPv6)</description>
			<testdata format="utf16le">MQA5ADIALgAxADYAOAAuADEALgAyADUANAANAAoAWwAyADAAMAAxADoAMABkAGIAOAA6ADgANQBhADMAOgAwADAAMAAwADoAMAAwADAAMAA6ADgAYQAyAGUAOgAwADMANwAwADoANwAzADMANABdAA0ACgAxADkAMgAuACAAMQA2ADgALgAxAC4AMgA1ADQADQAKADEAOQAyADoAMQA2ADgALgAxAC4AMgA1ADQADQAKADEAOQAyAC4AYQA2ADgALgAxAC4AMgA1ADQADQAKAA==</testdata>
		</action>
		<action id="HeaderVia" freespacing="1" flavor="pcre" flavorreplace="jgsoft" testscope="line">
			<regex>^(?i) &#13;&#10;# First relay&#13;&#10;(&#13;&#10;# Allow hostname at the beginning (not valid, but seen in practice)&#13;&#10;[-._0-9a-z]+&#13;&#10;|&#13;&#10;# Optional protocol name, then version&#13;&#10;(?:HTTP/)? [0-9.]+&#13;&#10;# received by (as hostname or pseudonim)&#13;&#10;\s+ [ -:._0-9a-z]+&#13;&#10;# optional comment&#13;&#10;(\s+\([ ():/.0-9a-z]+\))?&#13;&#10;)&#13;&#10;# Other relays &#13;&#10;(,\s+&#13;&#10;# Optional protocol name, then version&#13;&#10;(?:HTTP/)? [0-9.]+&#13;&#10;# received by (as hostname or pseudonim)&#13;&#10;\s+ [-:._0-9a-z]+&#13;&#10;# optional comment&#13;&#10;(\s+\([ ():/.0-9a-z]+\))?&#13;&#10;){0,}&#13;&#10;&#13;&#10;$</regex>
			<description>Header: Via</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="HeaderXForwardedFor" flavor="pcre" flavorreplace="jgsoft" testscope="line">
			<regex>(?i)^(?:[a-z]+|(?:(?:[0-9.]+|\[[:a-z0-9]+\])(,\s*(?:[0-9.]+|\[[:a-z0-9]+\])){0,}))$</regex>
			<description>Header: X-Forwarded-For</description>
			<testdata format="utf16le">dQBuAGsAbgBvAHcAbgANAAoAMQA5ADIALgAxADYAOAAuADIAMAAuADEAOAA2AA0ACgA0ADEALgAyADQAMAAuADIAMAA3AC4ANgAyACwAIAAxADkAOAAuADUANAAuADIAMAAyAC4AMQA4ADIADQAKAA==</testdata>
		</action>
		<action id="SQLiMSSQLStoredprocedureexecution" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)exec(?:ute)?[\[.\s]+(?:@[^=]*=[\s\[\].]*)?(?:(sys|master|master[\s\[\].]+dbo|dbo))?[\s.\[\]]*[sx]p_</regex>
			<description>SQLi: MSSQL: Stored procedure execution</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="SQLiMSSQLStoredprocedureexecutionnoexplicitidentifiers" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)exec(?:ute)?[\[.\s]+(?:@[^=]*=[\s\[\].]*)?.*[\s.\[\]]*([sx]p_|sysmail)</regex>
			<description>SQLi: MSSQL: Stored procedure execution, no explicit identifiers</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="SQLiMSSQLStoredprocedureexecutionviaavariable" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)exec(?:ute)?\s*[\(\[@\.]</regex>
			<description>SQLi: MSSQL: Stored procedure execution, via a variable</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="SQLiPostgreSQLCREATEFUNCTION" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)create(:?\s+or\s+replace)?\s+function\b</regex>
			<description>SQLi: PostgreSQL: CREATE FUNCTION</description>
			<testdata format="utf16le">QwBSAEUAQQBUAEUAIABGAFUATgBDAFQASQBPAE4AIAB0AGUAcwB0ACgAKQAgAFIARQBUAFUAUgBOAFMAIABpAG4AdABlAGcAZQByACAAQQBTACAAJAAkAA0ACgBDAFIARQBBAFQARQAgAE8AUgAgAFIARQBQAEwAQQBDAEUAIABGAFUATgBDAFQASQBPAE4AIAB0AGUAcwB0ACgAKQAgAFIARQBUAFUAUgBOAFMAIABpAG4AdABlAGcAZQByACAAQQBTACAAJAAkAA==</testdata>
		</action>
	</libraryfile>
</rxb:regexbuddy>
