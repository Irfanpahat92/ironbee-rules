<?xml version="1.0" encoding="UTF-8"?>
<rxb:regexbuddy xmlns:rxb="http://www.regexbuddy.com/regexbuddy32.xsd" version="3.2">
	<libraryfile>
		<action id="SQLiMSSQLStoredprocedureexecution" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)exec(?:ute)?[\[.\s]+(?:@[^=]*=[\s\[\].]*)?(?:(sys|master|master[\s\[\].]+dbo|dbo))?[\s.\[\]]*[sx]p_</regex>
			<description>SQLi: MSSQL: Stored procedure execution</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="SQLiMSSQLStoredprocedureexecutionnoexplicitidentifiers" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)exec(?:ute)?[\[.\s]+(?:@[^=]*=[\s\[\].]*)?.*[\s.\[\]]*([sx]p_|sysmail)</regex>
			<description>SQLi: MSSQL: Stored procedure execution, no explicit identifiers</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="SQLiMSSQLStoredprocedureexecutionviaavariable" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)exec(?:ute)?\s*[\(\[@\.]</regex>
			<description>SQLi: MSSQL: Stored procedure execution, via a variable</description>
			<testdata format="utf16le"></testdata>
		</action>
		<action id="SQLiPostgreSQLCREATEFUNCTION" flavor="pcre" flavorreplace="jgsoft">
			<regex>(?i)create(:?\s+or\s+replace)?\s+function\b</regex>
			<description>SQLi: PostgreSQL: CREATE FUNCTION</description>
